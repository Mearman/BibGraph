{
  "name": "@bibgraph/client",
  "description": "TypeScript client for the OpenAlex API with extensive entity support and utilities",
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mearman/BibGraph.git",
    "directory": "packages/client"
  },
  "scripts": {
    "build": "nx run client:build",
    "clean": "nx run client:clean",
    "dev": "nx run client:dev",
    "lint": "nx run client:lint",
    "prepublishOnly": "nx run client:prepublishOnly",
    "test": "nx run client:test",
    "test:coverage": "nx run client:test:coverage",
    "test:integration": "nx run client:test:integration",
    "test:unit": "nx run client:test:unit",
    "test:watch": "nx run client:test:watch",
    "typecheck": "nx run client:typecheck"
  },
  "dependencies": {
    "@bibgraph/types": "workspace:*",
    "@bibgraph/utils": "workspace:*",
    "dexie": "4.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "4.0.14",
    "eslint": "9.39.1",
    "immer": "11.0.0",
    "memfs": "4.47.0",
    "msw": "2.12.3",
    "typescript": "5.9.3",
    "vitest": "4.0.14",
    "vitest-axe": "0.1.0"
  },
  "author": "BibGraph",
  "bugs": "https://github.com/Mearman/BibGraph/issues",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/index.d.ts",
      "source": "./src/client.ts",
      "import": "./dist/index.js"
    },
    "./cached-client": {
      "types": "./dist/index.d.ts",
      "source": "./src/cached-client.ts",
      "import": "./dist/index.js"
    },
    "./internal/static-data-provider": {
      "types": "./dist/index.d.ts",
      "source": "./src/internal/static-data-provider.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "homepage": "https://github.com/Mearman/BibGraph/tree/main/packages/client#readme",
  "keywords": [
    "academic",
    "api-client",
    "authors",
    "citations",
    "institutions",
    "openalex",
    "publications",
    "research",
    "scholarly",
    "typescript"
  ],
  "license": "MIT",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "nx": {
    "includedScripts": []
  },
  "types": "./dist/src/index.d.ts"
}
