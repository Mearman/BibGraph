{
  "name": "@bibgraph/algorithms",
  "description": "Zero-dependency TypeScript graph algorithms: DFS, BFS, Dijkstra, topological sort, cycle detection, connected components, and strongly connected components. Generic types support heterogeneous graphs with O(V+E) performance.",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "nx run algorithms:build",
    "clean": "nx run algorithms:clean",
    "lint": "nx run algorithms:lint",
    "test": "nx run algorithms:test",
    "test:coverage": "nx run algorithms:test:coverage",
    "test:integration": "nx run algorithms:test:integration",
    "test:unit": "nx run algorithms:test:unit",
    "test:watch": "nx run algorithms:test:watch",
    "typecheck": "nx run algorithms:typecheck"
  },
  "dependencies": {
    "@bibgraph/graph-expansion": "workspace:*"
  },
  "devDependencies": {},
  "author": "BibGraph",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "academic-research",
    "adjacency-list",
    "algorithms",
    "bfs",
    "citation-network",
    "connected-components",
    "cycle-detection",
    "dfs",
    "dijkstra",
    "directed-graph",
    "generic-types",
    "graph",
    "graph-algorithms",
    "graph-theory",
    "network-analysis",
    "pathfinding",
    "scc",
    "strongly-connected-components",
    "tarjan",
    "topological-sort",
    "traversal",
    "typescript",
    "undirected-graph",
    "zero-dependencies"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "nx": {
    "includedScripts": []
  },
  "types": "./dist/index.d.ts"
}
