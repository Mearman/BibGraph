{
  "name": "evaluation",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/evaluation/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/evaluation",
        "configFile": "packages/evaluation/vite.config.ts"
      },
      "configurations": {
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/packages/evaluation"],
      "options": {
        "configFile": "packages/evaluation/vitest.config.ts",
        "passWithNoTests": true
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p packages/evaluation/tsconfig.json",
        "cwd": "."
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/evaluation/**/*.{ts,tsx}"]
      }
    }
  }
}
