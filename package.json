    "eslint-plugin-unicorn": "62.0.0",
    "eslint-plugin-unused-imports": "4.2.0",
    "eslint-plugin-yml": "1.19.0",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jiti": "2.6.1",
    "jsdom": "27.2.0",
    "knip": "5.70.2",
    "koa": "3.1.1",
    "lint-staged": "16.2.1",
    "monorepo-license-checker": "0.3.0",
    "msw": "2.12.3",
    "nx": "22.1.3",
    "prettier": "3.7.4",
    "semantic-release": "25.0.2",
    "syncpack": "13.0.4",
    "tslib": "2.8.1",
    "tsx": "4.21.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.48.0",
    "vite": "7.2.4",
    "vite-plugin-dts": "4.5.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.14",
    "vitest-axe": "0.1.0",
    "zod": "4.1.13"
  },
  "nx": {
    "includedScripts": []
  },
  "pnpm": {
    "overrides": {
      "koa": "3.1.1",
      "glob": "11.1.0",
      "body-parser": "2.2.1"
    }
  }
}
