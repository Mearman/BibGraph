{
  "name": "bibgraph",
  "version": "24.3.0",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "repository": "https://github.com/Mearman/BibGraph.git",
  "scripts": {
    "build": "nx run-many -t build",
    "clean": "nx reset && rm -rf dist coverage .nx/cache .tanstack node_modules/.vite && pnpm --filter='*' clean",
    "cli": "tsx apps/cli/src/openalex-cli.ts",
    "coverage:report": "echo 'Coverage reports already generated during test execution'",
    "deps:analyze": "nx show project web --json | jq '.implicitDependencies' && nx graph --file=deps-graph.json && echo 'Dependency graph generated'",
    "deps:audit": "nx run bibgraph:deps:audit",
    "deps:knip": "nx run bibgraph:deps:knip",
    "deps:knip:fix": "nx run bibgraph:deps:knip:fix",
    "deps:outdated": "nx run bibgraph:deps:outdated",
    "deps:update": "pnpm update --interactive --latest",
    "dev": "nx serve web",
    "generate:test-urls": "tsx scripts/generate-test-urls.ts",
    "kill-nx": "tsx scripts/kill-nx-processes.ts",
    "kill-nx:emergency": "pkill -f nx; pkill -f '@nx/daemon'; rm -rf .nx /tmp/nx* /tmp/*nx* 2>/dev/null || true; nx reset",
    "licenses:check": "tsx tools/scripts/check-licenses.ts",
    "licenses:generate": "monorepo-license-checker --markdown --exclude-private-packages",
    "licenses:summary": "monorepo-license-checker --exclude-private-packages",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint:fix",
    "prepare": "husky",
    "preview": "nx run web:preview",
    "release": "nx release",
    "release:dry": "nx release --dry-run",
    "release:publish": "nx release publish",
    "release:version": "nx release version",
    "speckit": "uvx --from git+https://github.com/github/spec-kit.git specify",
    "sync:targets": "tsx tools/generators/sync-targets/cli.ts",
    "sync:targets:dry": "tsx tools/generators/sync-targets/cli.ts --dry-run",
    "syncpack": "nx run bibgraph:syncpack",
    "syncpack:fix": "nx run bibgraph:syncpack:fix",
    "syncpack:lint": "nx run bibgraph:syncpack:lint",
    "syncpack:list": "nx run bibgraph:syncpack:list",
    "test": "NODE_OPTIONS='--max-old-space-size=6144' NX_DAEMON=false timeout 300 nx run-many -t test --output-style=stream --maxParallel=2",
    "test:accessibility": "nx run web:test:accessibility",
    "test:accessibility:dev": "nx run web:test:accessibility:dev",
    "test:aggregate": "tsx scripts/aggregate-tests.ts",
    "test:performance": "nx run web:test:performance",
    "test:urls": "tsx scripts/test-urls.ts",
    "test:web": "nx run web:test",
    "typecheck": "nx run-many -t typecheck",
    "typecheck:ci": "nx run-many -t typecheck --verbose",
    "typecheck:fast": "tsx scripts/fast-typecheck.ts",
    "validate": "nx run-many -t validate"
  },
  "devDependencies": {
    "@commitlint/cli": "20.0.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint-react/eslint-plugin": "2.3.11",
    "@eslint/js": "9.39.1",
    "@eslint/markdown": "7.5.1",
    "@nx/devkit": "22.1.3",
    "@nx/eslint": "22.1.3",
    "@nx/eslint-plugin": "22.1.3",
    "@nx/js": "22.1.3",
    "@nx/playwright": "22.1.3",
    "@nx/react": "22.1.3",
    "@nx/rollup": "22.1.3",
    "@nx/storybook": "22.1.3",
    "@nx/vite": "22.1.3",
    "@nx/vitest": "22.1.3",
    "@nx/web": "22.1.3",
    "@octokit/rest": "22.0.1",
    "@playwright/test": "1.57.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "12.0.2",
    "@semantic-release/npm": "13.1.2",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@stylistic/eslint-plugin": "5.4.0",
    "@swc-node/register": "1.11.1",
    "@swc/core": "1.15.1",
    "@swc/helpers": "0.5.17",
    "@tanstack/eslint-plugin-query": "5.91.2",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/d3": "7.4.3",
    "@types/d3-force": "3.0.10",
    "@types/d3-random": "3.0.3",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/three": "0.181.0",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "@typescript-eslint/utils": "8.46.3",
    "@vanilla-extract/vite-plugin": "5.1.1",
    "@vitejs/plugin-react": "5.1.1",
    "@vitest/coverage-v8": "4.0.14",
    "@vitest/eslint-plugin": "1.5.1",
    "@vitest/ui": "4.0.14",
    "chokidar": "5.0.0",
    "eslint": "9.39.1",
    "eslint-config-xo": "0.49.0",
    "eslint-config-xo-typescript": "9.0.0",
    "eslint-formatter-compact": "9.0.1",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-autofix": "2.2.0",
    "eslint-plugin-barrel-files": "3.0.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "61.4.1",
    "eslint-plugin-jsonc": "2.21.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-playwright": "2.4.0",
    "eslint-plugin-prefer-arrow-functions": "3.9.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.22",
    "eslint-plugin-regexp": "2.10.0",
